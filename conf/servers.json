{
    "local": {
        "mongodb": {
            "port": 3001
        }
    },
    "production": {
        "vHost": {
            "apache24": true,       
            "baseFile": "assets/vhost.txt",
            "destDir": "/etc/apache2/sites-available"
        },
        "mongodb": {
            "rootUser": "root",
            "rootPass": "rootPass",
            "rootAuthDb": "admin",
            "user": "dbUser",
            "pass": "dbPass",
            "address": "x.x.x.x",
            "port": 27017,
            "dbName": "domain"
        },
        "domain": "www.domain.com",
        "webDir": "/var/www/user/domain.com",
        "port": 3000,
        "foreverProcessName": "app-name",
        "sshUser": "sshUser",
        "sshUserHome": "/home/sshUser",
        "sshAddress": "x.x.x.x",
        "sshPass": "sshPass",
        "localRunLog": "/your/desired/folder/running-apps.txt",
        "remoteRunLog": "/your/desired/folder/running-apps.txt"
    }
}